#include <stdio.h>
#include <string.h>
void main()
{
	char yuanshi[] = "2+2*3+2/2-1";
	char gaishi[100];
	gaishi[strlen(yuanshi)] = '\0';
	int  a, c, b = 0,i;
	for (i = 0;i < strlen(yuanshi);i++)
	{
		if (yuanshi[i] == '*')
		{
			a=yuanshi[i-1] - '0';
			c = yuanshi[i + 1] - '0';
			gaishi[i-2*b-1] = a*c + '0';
			
			i++;b++;
		}
		else
		{
			if (yuanshi[i] == '/')
			{
				a = yuanshi[i-1] - '0';
				c = yuanshi[i + 1] - '0';
				gaishi[i-2*b-1] = a / c + '0';
				
				i++;b++;
			}
			else
			{
				gaishi[i-2*b] = yuanshi[i];
			}
		}
		
	}printf("%s\n",gaishi);
	gaishi[7] = '\0';
	int j, sum = gaishi[0]-'0';
	for (j = 0;j < 7;j++)
	{
		if (gaishi[j + 1] == '+' || gaishi[j + 1] == '-')
		{
			if (gaishi[j + 1] == '+')
				sum = sum + gaishi[j + 2] - '0';
			else
				sum = sum - (gaishi[j + 2] - '0');

		}
	}printf("%d\n", sum);
	
}
